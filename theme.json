{
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "Fira Code, Consolas",
    "editor.fontLigatures": true,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.colorCustomizations": {
        "[Solarized Dark]": {
            "editor.background": "#00252e",
            "sideBar.background": "#081121"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Solarized Dark]": {
            "variables": "#328bff",
            "functions": "#f3b14e",
            // "functions": "#d6bf58",
            "textMateRules": [
                {
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#84df9b"
                    }
                },
                {
                    "scope": ["variable.name", "variable.language", "variable.other"],
                    "settings": {
                        "foreground": "#84df9b",
                    }
                }
            ]
        }
    }
}